{
    "name": "拡張機能の名称",
    "description": "拡張機能の説明",
    "version": "1.0.0",
    "action": {},
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "js/content_script.js"
            ]
        }
    ]
}